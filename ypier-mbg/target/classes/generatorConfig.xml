        <sqlMapGenerator targetPackage="com.macro.mall.mapper" targetProject="ypier-mbg\src\main\resources"/>

        <table tableName="%">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
    </context>
</generatorConfiguration>-->
